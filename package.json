{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "node express typescript , sequelize orm and msql boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/server.ts",
    "build": "tsc",
    "start": "node dist/src/server.js",
    "db:migrate": "ts-node node_modules/.bin/sequelize db:migrate",
    "db:migrate:rollback": "ts-node node_modules/.bin/sequelize db:migrate:undo:all",
    "db:seed": "ts-node node_modules/.bin/sequelize db:seed:all",
    "seed": "npx sequelize-cli db:seed:all --seeders-path ./src/database/seeders"
  },
  "keywords": [
    "node",
    "express",
    "typescript",
    "sequelize",
    "mysql"
  ],
  "author": "marko novakovic",
  "license": "ISC",
  "dependencies": {
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-request": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "mysql2": "^2.3.2",
    "node": "^16.11.1",
    "nodemon": "^2.0.14",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.8.0",
    "sequelize-auto": "^0.8.6",
    "sequelize-typescript": "^2.1.1",
    "tsyringe": "^4.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/node": "^16.11.1",
    "@types/sequelize": "^4.28.10",
    "@types/validator": "^13.6.4",
    "sequelize-cli": "^5.5.1",
    "ts-node": "^10.3.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
